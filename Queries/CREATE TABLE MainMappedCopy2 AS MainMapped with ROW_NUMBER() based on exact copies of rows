CREATE TABLE MainMappedCopy2 AS
SELECT *, 
    ROW_NUMBER() OVER (PARTITION BY ID, RECORDED_TIME, GENDER, AGE, RACE_NAME, ETHNICITY, ENC_TYPE_NAME, DEPARTMENT_ID, CAD, CHF, COPD, DIABETES, HYPERTENSION, RESULTS, QUESTION_ID, QUESTION_NAME, DOCUMENTATION_SOURCE_NAME, EMPY_STATUS_NAME, EDU_LEVEL_NAME, TOBACCO_USER_NAME, RESULTS_NAME ORDER BY (SELECT NULL)) AS RowNum
FROM MainMapped;